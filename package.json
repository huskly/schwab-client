{
  "name": "@huskly/schwab-client",
  "version": "0.2.0",
  "description": "Schwab Market Data and Trading API client",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.ts' 'package.json' 'tsconfig.json'",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "schwab",
    "trading",
    "api",
    "market-data",
    "stocks",
    "options"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "typescript": "^5.7.2"
  }
}
