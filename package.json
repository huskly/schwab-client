{
  "name": "@huskly/schwab-client",
  "version": "0.3.0",
  "description": "Schwab Market Data and Trading API client",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format:write": "prettier --write 'src/**/*.ts' 'package.json' 'tsconfig.json'",
    "format:check": "prettier --check 'src/**/*.ts' 'package.json' 'tsconfig.json'",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "schwab",
    "trading",
    "api",
    "market-data",
    "stocks",
    "options"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.54.0"
  }
}
